# Configurações de Produção - MDBot Dashboard
NODE_ENV=production
VITE_API_URL=https://mdbot-backend.squareweb.app/api
VITE_DISCORD_REDIRECT_URI=https://mdbot-dashboard.squareweb.app/auth/callback
VITE_DISCORD_CLIENT_ID=seu_client_id_aqui

# Otimizações de Build
VITE_COMPRESSION=true
VITE_CACHE_CONTROL=true
VITE_CDN_ENABLED=false

# Configurações de Performance
VITE_LAZY_LOADING=true
VITE_CODE_SPLITTING=true
VITE_PREFETCH=true

# Analytics e Monitoramento
VITE_ANALYTICS_ID=seu_analytics_id
VITE_SENTRY_DSN=seu_sentry_dsn

# Segurança
VITE_CSP_ENABLED=true
VITE_HSTS_ENABLED=true